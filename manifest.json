{
  "name": "BBN Patcher",
  "version": "0.1",
  "manifest_version": 2,
  "description": "The official BBN browser extension.",
  "homepage_url": "https://bbn.one/",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "BBN"
  }
}
