{
  "name": "BigBotNetwork",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The official BigBotNetwork browser extension.",
  "homepage_url": "https://bigbotnetwork.com/",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "BigBotNetwork"
  }
}
