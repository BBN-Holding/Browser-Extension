{
  "manifest_version": 2,

  "name": "BigBotNetwork",
  "description": "The official BigBotNetwork browser extension.",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },

  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "BigBotNetwork"
  }
}
